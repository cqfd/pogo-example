{
  "name": "pogo-example",
  "version": "1.0.0",
  "scripts": {
    "watch-client": "watchify client/client.js -o app/assets/javascripts/bundle.js --verbose",
    "postinstall": "browserify client/client.js -o app/assets/javascripts/bundle.js"
  },
  "browserify": {
    "verbose": true,
    "transform": [
      [
        "babelify", { "optional": ["runtime"] }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/happy4crazy/pogo-example.git"
  },
  "dependencies": {
    "bluebird": "*",
    "lodash": "*",
    "react": "*",

    "babel": "*",
    "babel-runtime": "*",
    "babelify": "*",
    "browserify": "*"
  },
  "devDependencies": {
    "watchify": "*"
  }
}
